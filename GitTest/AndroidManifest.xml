<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skycopyhot.gittest"
    android:versionCode="2"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.skycopyhot.gittest.activity.GitActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.skycopyhot.gittest.activity.TestInputActivity"
            android:label="@string/test_input"
            android:windowSoftInputMode="stateVisible|adjustResize" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.skycopyhot.gittest.GITTEST_SKY" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.skycopyhot.gittest.activity.TestSwipeListView"
            android:label="@string/test_swipe" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.skycopyhot.gittest.GITTEST_SKY"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
